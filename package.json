{
  "name": "tedy",
  "version": "0.0.5-beta",
  "description": "Your sweet Terminal Editor pal",
  "source": "src/index.tsx",
  "main": "dist/index.js",
  "bin": "index.js",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "tsx src/index.tsx",
    "build": "yarn format && rollup -c --environment NODE_ENV:production",
    "watch": "rollup -c -w",
    "format": "deno fmt --ignore=dist && deno lint --ignore=dist",
    "prebuild": "patch-package"
  },
  "author": {
    "name": "Jeroen Peeters",
    "email": "contact@jeroenpeeters.be"
  },
  "license": "MIT",
  "dependencies": {
    "@jeroenpeeters/assert-errors": "^1.0.2",
    "cli-cursor": "^4.0.0",
    "cli-highlight": "^2.1.11",
    "ink": "https://gitpkg.now.sh/jeroenptrs/tedy/vendor/ink?main",
    "react": "^18.2.0",
    "term-size": "^3.0.2",
    "tslib": "^2.6.1"
  },
  "devDependencies": {
    "@jeroenpeeters/tsconfig": "^1.1.0",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.20",
    "patch-package": "^8.0.0",
    "rollup": "^3.28.1",
    "rollup-plugin-cleandir": "^2.0.0",
    "tsx": "^3.12.7",
    "type-fest": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
